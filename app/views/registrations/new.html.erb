<h1>Create your account</h1>
<% if flash[:alert] %>
  <%= flash[:alert] %>
<% end %>
<%= form_for @user do |f| %>

  <%= f.label :username %>
  <%= f.text_field :username %>
  <br>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <br>
  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %>
  <br>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br>
  <%= f.label :venmo %>
  <%= f.text_field :venmo %>
  <br>
  <%= f.submit %>

<% end %>
